{
  "name": "fhs-react-start-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "test": "react-scripts test",
    "start": "concurrently -n app,storybook 'yarn start:app' 'yarn start:storybook'",
    "start:app": "react-scripts start",
    "start:storybook": "start-storybook -p 9009 -s public",
    "build": "concurrently -n app,storybook 'yarn build:app' 'yarn build:storybook'",
    "build:app": "react-scripts build",
    "build:storybook": "build-storybook -s storybook",
    "lint": "semistandard"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.4",
    "@storybook/addon-links": "^5.0.4",
    "@storybook/addons": "^5.0.4",
    "@storybook/react": "^5.0.4",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "semistandard": "^13.0.1"
  }
}
